nt!<br></br>\r\n                      <Button\r\n                        style={{ margin: \"10px\" }}\r\n                        variant=\"secondary\"\r\n                        onClick={closeModal}\r\n                        onChange={handleLogOut}\r\n                      >\r\n                        No,Wait\r\n                      </Button>\r\n                      <Button\r\n                        style={{\r\n                          margin: \"10px\",\r\n                          backgroundColor: \"#ef6e25\",\r\n                          border: \"#ef6e25\",\r\n                          borderRadius: \"10%\",\r\n                        }}\r\n                        variant=\"primary\"\r\n                        onClick={handleLogOut}\r\n                      >\r\n                        Yes,Logout\r\n                      </Button>\r\n                    </div>\r\n                  </Fade>\r\n                </Modal>\r\n              </>\r\n            ) : (\r\n              <>\r\n                <Button1 onClick={handleLogin}>\r\n                  <a\r\n                    href={`${config.drupal_url}/user/login`}\r\n                    style={{ color: \"#f5f5f5\" }}\r\n                  >\r\n                    LOGIN\r\n                  </a>\r\n                </Button1>\r\n              </>\r\n            )}\r\n          </LinkWrapper>\r\n        </Menu1>\r\n      </Container>\r\n    </Nav>\r\n  );\r\n};\r\n\r\nexport default Navbar;\r\nconst Container = styled.div`\r\n  display: flex;\r\n  justify-content: space-between;\r\n  width: 100%;\r\n  align-items: center;\r\n  flex-wrap: wrap;\r\n  max-width: 100%;\r\n  margin: auto;\r\n  padding: 0.5%;\r\n  img {\r\n    cursor: Pointer;\r\n  }\r\n  a {\r\n    text-decoration: none;\r\n    color: #000000;\r\n    font-size: 18px;\r\n    font-family: Montserrat;\r\n    padding: 0.7rem 1.5rem;\r\n    transition: all 0.2s ease-in;\r\n    font-wegiht: 500;\r\n    &:hover {\r\n      color: #ef6e25;\r\n    }\r\n  }\r\n`